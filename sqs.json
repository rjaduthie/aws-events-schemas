{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "https://duthie.cloud/aws/events/sqs.json",
  "type": "object",
     "title": "AWS SQS Message Schema",
     "required": [
       "Records"
     ],
    "properties": {
      "Records": {
        "$id": "#/properties/Records",
        "type": "array",
        "title": "The Records Schema",
        "items": {
          "$id": "#/properties/Records/items",
          "type": "object",
          "title": "The Items Schema",
          "required": [
            "messageId",
            "receiptHandle",
            "body",
            "attributes",
            "messageAttributes",
            "md5OfBody",
            "eventSource",
            "eventSourceARN",
            "awsRegion"
          ],
          "properties": {
            "messageId": {
              "$id": "#/properties/Records/items/properties/messageId",
              "type": "string",
              "default": "",
              "examples": [
                "c80e8021-a70a-42c7-a470-796e1186f753"
              ],
              "pattern": "^([a-z0-9-]*)$"
            },
            "receiptHandle": {
              "$id": "#/properties/Records/items/properties/receiptHandle",
              "type": "string",
              "default": "",
              "examples": [
                ""
              ],
              "pattern": "^([a-zA-Z0-9/+=]*)$"
            },
            "body": {
              "$id": "#/properties/Records/items/properties/body",
              "type": "string",
              "default": "",
              "examples": [
                "{\"foo\":\"bar\"}"
              ],
              "pattern": "^(.*)$"
            },
            "attributes": {
              "$id": "#/properties/Records/items/properties/attributes",
              "type": "object",
              "required": [
                "ApproximateReceiveCount",
                "SentTimestamp",
                "SenderId",
                "ApproximateFirstReceiveTimestamp"
              ],
              "properties": {
                "ApproximateReceiveCount": {
                  "$id": "#/properties/Records/items/properties/attributes/properties/ApproximateReceiveCount",
                  "type": "string",
                  "default": "",
                  "examples": [
                    "3"
                  ],
                  "pattern": "^(.*)$"
                },
                "SentTimestamp": {
                  "$id": "#/properties/Records/items/properties/attributes/properties/SentTimestamp",
                  "type": "string",
                  "default": "",
                  "examples": [
                    "1529104986221"
                  ],
                  "pattern": "^(.*)$"
                },
                "SenderId": {
                  "$id": "#/properties/Records/items/properties/attributes/properties/SenderId",
                  "type": "string",
                  "default": "",
                  "examples": [
                    "594035263019"
                  ],
                 "pattern": "^(.*)$"
               },
               "ApproximateFirstReceiveTimestamp": {
                 "$id": "#/properties/Records/items/properties/attributes/properties/ApproximateFirstReceiveTimestamp",
                 "type": "string",
                 "default": "",
                 "examples": [
                   "1529104986230"
                 ],
                 "pattern": "^(.*)$"
               }
             }
           },
           "messageAttributes": {
             "$id": "#/properties/Records/items/properties/messageAttributes",
             "type": "object",
           },
           "md5OfBody": {
             "$id": "#/properties/Records/items/properties/md5OfBody",
             "type": "string",
             "default": "",
             "examples": [
               "9bb58f26192e4ba00f01e2e7b136bbd8"
             ],
             "pattern": "^([a-z0-9]*)$"
           },
           "eventSource": {
             "$id": "#/properties/Records/items/properties/eventSource",
             "type": "string",
             "default": "",
             "examples": [
               "aws:sqs"
             ],
             "pattern": "^(.*)$"
          },
           "eventSourceARN": {
             "$id": "#/properties/Records/items/properties/eventSourceARN",
             "type": "string",
             "default": "",
             "examples": [
               "arn:aws:sqs:us-west-2:594035263019:NOTFIFOQUEUE"
             ],
             "pattern": "^([a-zA-Z0-9:-]*)$"
           },
          "awsRegion": {
              "$id": "#/properties/Records/items/properties/awsRegion",
              "type": "string",
              "default": "",
              "examples": [
                "us-west-2"
              ],
              "pattern": "^([a-z0-9-]*)$"
            }
          }
        }
      }
   }
}
